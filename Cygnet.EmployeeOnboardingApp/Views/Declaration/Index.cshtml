@model IEnumerable<Cygnet.EmployeeOnboardingApp.Data.Model.Declaration>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Kyc_Details)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Is_Mem_Epf_Schema)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dt_Allot_Ins_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dt_Issue_Tic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modified)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RowVersion)
            </th>
            <th></th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Kyc_Details)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Is_Mem_Epf_Schema)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dt_Allot_Ins_No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dt_Issue_Tic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RowVersion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    </table>
</body>
</html>